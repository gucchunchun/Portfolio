---
import SubContentLayout from '~/layouts/SubContentLayout.astro';
import Marquee from '~/components/ui/Marquee.astro';
import type { MarqueeSubContent as Props } from '~/types';

const {
  items = [],
  maxWidth = undefined,
  ...other
} = Astro.props;
---

<SubContentLayout
  {...other}
>
  <Marquee 
    items={items}
    maxWidth={maxWidth}
  />    
</SubContentLayout>